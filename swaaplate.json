{
  "backend": {
    "language": "php",
    "javaKt": {
      "management": "",
      "packagePath": ""
    },
    "php": {
      "modRewritePhpExtension": true,
      "runAsApi": true
    }
  },
  "frontend": {
    "buildDir": "dist",
    "ghUser": "inpercima",
    "installDependencies": true,
    "language": "en",
    "modules": {
      "enabled": true,
      "features": {
        "defaultRoute": "dashboard",
        "name": "features"
      },
      "notFound": {
        "enabled": false,
        "name": ""
      },
      "routing": false
    },
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "http://publicmedia.inpercima.net/",
      "repository": "https://github.com/inpercima/publicmedia/"
    },
    "prefix": "pm",
    "theme": "indigo-pink",
    "useGoogleFonts": true,
    "useYarn": true
  },
  "general": {
    "author": "Marcel Jänicke",
    "description": "A tool that shows how public data can be accessed from Instagram without an Instagram account.",
    "modRewriteIndex": true,
    "name": "publicmedia",
    "title": "publicmedia",
    "useDocker": false,
    "useMITLicense": true,
    "useMock": false,
    "useSecurity": false
  }
}
